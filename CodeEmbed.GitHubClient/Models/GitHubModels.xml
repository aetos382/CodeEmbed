<?xml version="1.0" encoding="utf-8" ?>
<Models codeNamespace="CodeEmbed.GitHubClient">

  <Model name="Repository" hasClient="true">

    <Member name="Id" type="long" json="id"/>
    <Member name="Owner" type="RepositoryUser" json="owner" required="true"/>
    <Member name="Name" type="string" json="name"/>
    <Member name="FullName" type="string" json="full_name"/>
    <Member name="Description" type="string" json="description"/>
    <Member name="IsPrivate" type="bool" json="private"/>
    <Member name="Fork" type="bool" json="fork"/>
    <Member name="Uri" type="string" json="url"/>
    
  </Model>

  <Model name="RepositoryUser">

    <Member name="Name" type="string" json="name" required="true"/>
    <Member name="Uri" type="string" json="url"/>

  </Model>

  <Model name="Foo" inherits="RepositoryUser">

    <Member name="Hoge" type="string" json="hoge"/>

  </Model>

  <Model name="Bar" inherits="Foo">

    <Member name="Hige" type="string" json="hige"/>

  </Model>

</Models>
