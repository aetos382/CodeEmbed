<?xml version="1.0" encoding="utf-8" ?>
<g:Models
  codeNamespace="CodeEmbed.GitHubClient"
  xmlns="foo"
  xmlns:g="http://schemas.aerie.jp/codeembed/github-models-v3"
  xmlns:gi="http://schemas.aerie.jp/codeembed/github-models-v3-instance"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://schemas.aerie.jp/codeembed/github-models-v3 GitHubModels.xsd">

  <g:Model name="gi:PublicRepository" hasClient="true">

    <g:Member name="Id" type="long"/>
    <g:Member name="Owner" type="gi:RepositoryUser" required="true"/>
    <g:Member name="Name" type="string" required="true"/>
    <g:Member name="FullName" type="string"/>
    <g:Member name="Description" type="string"/>
    <g:Member name="IsPrivate" type="bool" json="private"/>
    <g:Member name="Fork" type="bool"/>
    <g:Member name="Uri" type="Uri" json="url"/>
    <g:Member name="HtmlUri" type="Uri" json="html_url"/>

  </g:Model>
  
  <g:Model name="gi:Repository" hasClient="true">

    <g:Inherits name="gi:PublicRepository"/>
    
    <g:Member name="CloneUri" type="Uri"/>
    <g:Member name="GitUri" type="Uri" json="git_url"/>
    <g:Member name="SshUri" type="Uri" json="ssh_url"/>
    <g:Member name="SubversionUri" type="Uri" json="svn_url"/>
    <g:Member name="MirrorUri" type="Uri" json="mirror_url"/>
    <g:Member name="Homepage" type="Uri"/>
    <g:Member name="Language" type="string"/>
    <g:Member name="ForksCount" type="int"/>
    <g:Member name="StargazersCount" type="int"/>
    <g:Member name="WatchersCount" type="int"/>
    <g:Member name="Size" type="long"/>
    <g:Member name="DefaultBranch" type="string" required="true"/>
    <g:Member name="OpenIssuesCount" type="int"/>
    <g:Member name="HasIssues" type="bool"/>
    <g:Member name="HasWiki" type="bool"/>
    <g:Member name="HasPages" type="bool"/>
    <g:Member name="HasDownloads" type="bool"/>
    <g:Member name="PushedAt" type="DateTime"/>
    <g:Member name="CreatedAt" type="DateTime"/>
    <g:Member name="UpdatedAt" type="DateTime"/>
    <g:Member name="Permissions" type="gi:Permissions"/>
    <g:Member name="SubscribersCount" type="int"/>
    <g:Member name="Organization" type="gi:RepositoryUser"/>
    <g:Member name="Parent" type="gi:Repository"/>
    <g:Member name="Source" type="gi:Repository"/>
  
  </g:Model>

  <g:Model name="gi:Permissions">
    
    <g:Member name="Administration" type="bool" json="admin"/>
    <g:Member name="Push" type="bool"/>
    <g:Member name="Pull" type="bool"/>
  
  </g:Model>
    
  <g:Model name="gi:RepositoryUser">

    <g:Member name="Login" type="string" required="true"/>
    <g:Member name="Id" type="long"/>
    <g:Member name="AvatarUri" type="Uri" json="avatar_url"/>
    <g:Member name="GravatarId" type="string"/>
    <g:Member name="Uri" type="Uri" json="url"/>
    <g:Member name="HtmlUri" type="Uri" json="html_url"/>
    <g:Member name="FollowersUri" type="Uri" json="followers_url"/>
    <g:Member name="FollowingUri" type="string" json="following_url"/>
    <g:Member name="GistsUri" type="string" json="gists_url"/>
    <g:Member name="StarredUri" type="string" json="starred_url"/>
    <g:Member name="SubscriptionsUri" type="Uri" json="subscriptions_url"/>
    <g:Member name="OrganizationsUri" type="Uri" json="organizations_url"/>
    <g:Member name="RepositoriesUri" type="Uri" json="repos_url"/>
    <g:Member name="EventUri" type="string" json="events_url"/>
    <g:Member name="ReceivedEventsUri" type="Uri" json="received_events_url"/>
    <g:Member name="Type" type="string"/>
    <g:Member name="SiteAdministrator" type="bool" json="site_admin"/>

  </g:Model>

  <g:Model name="gi:GistBase" abstract="true">
    <g:GenericParameter name="TGistFile" constraints="gi:GistFile"/>
    <g:Member name="Files" type="IDictionary" genericArguments="string TGistFile"/>
  </g:Model>
  
  <g:Model name="gi:GistSummary">

    <g:Inherits name="gi:GistBase"/>
    
    <g:Member name="Uri" type="Uri" json="url"/>
    <g:Member name="ForksUri" type="Uri" json="forks_url"/>
    <g:Member name="CommitsUri" type="Uri" json="commits_url"/>
    <g:Member name="Id" type="string"/>
    <g:Member name="Description" type="string"/>
    <g:Member name="IsPublic" type="bool" json="public"/>
    <g:Member name="Owner" type="gi:RepositoryUser"/>
    <g:Member name="User" type="string"/>
    <!--
    <g:Member name="Files" type="IDictionary&lt;string, TGistFile&gt;"/>
    -->
    <g:Member name="Comments" type="int"/>
    <g:Member name="CommentsUri" type="Uri" json="comments_url"/>
    <g:Member name="HtmlUri" type="Uri" json="html_url"/>
    <g:Member name="GitPullUri" type="Uri" json="git_pull_url"/>
    <g:Member name="GitPushUri" type="Uri" json="git_push_url"/>
    <g:Member name="CreatedAt" type="DateTime"/>
    <g:Member name="UpdatedAt" type="DateTime"/>

  </g:Model>

  <g:Model name="gi:Gist">

    <g:Inherits name="gi:GistBase"/>
    
    <g:Member name="Forks" type="IEnumerable" genericArguments="gi:GistFork"/>
    <g:Member name="Histories" type="IEnumerable" genericArguments="gi:GistHistory"/>

  </g:Model>

  <g:Model name="gi:GistFile">

    <g:Member name="Size" type="long"/>
    <g:Member name="RawUri" type="Uri" json="raw_url"/>
    <g:Member name="ContentType" type="string" json="type"/>
    <g:Member name="Truncated" type="bool"/>
    <g:Member name="Language" type="string"/>
  
  </g:Model>

  <g:Model name="gi:GistFileContent">

    <g:Inherits name="gi:GistFile"/>
    
    <g:Member name="Content" type="string"/>

  </g:Model>
  
  <g:Model name="gi:GistFork">

    <g:Member name="User" type="gi:RepositoryUser"/>
    <g:Member name="Uri" type="Uri" json="url"/>
    <g:Member name="Id" type="string"/>
    <g:Member name="CreatedAt" type="DateTime"/>
    <g:Member name="UpdatedAt" type="DateTime"/>
  
  </g:Model>
  
  <g:Model name="gi:GistHistory">
    
    <g:Member name="Uri" type="Uri" json="url"/>
    <g:Member name="Version" type="string"/>
    <g:Member name="User" type="gi:RepositoryUser"/>
    <g:Member name="ChangeStatus" type="gi:GistChangeStatus"/>
    <g:Member name="CommittedAt" type="DateTime"/>

  </g:Model>

  <g:Model name="gi:GistChangeStatus">

    <g:Member name="Deletions" type="int"/>
    <g:Member name="Additions" type="int"/>
    <g:Member name="Total" type="int"/>

  </g:Model>

  <g:Model name="gi:GitReference">

    <g:Member name="Reference" type="string" json="ref"/>
    <g:Member name="Uri" type="Uri" json="url"/>
    <g:Member name="Target" type="gi:GitObject" json="object"/>

  </g:Model>

  <g:Model name="gi:GitObject">

    <g:Member name="ObjectType" type="GitObjectType" json="type"/>
    <g:Member name="Hash" type="string" json="sha"/>
    <g:Member name="Uri" type="Uri" json="url"/>

  </g:Model>

  <g:Model name="gi:User">

    <g:Inherits name="gi:RepositoryUser"/>
    
    <g:Member name="Name" type="string"/>
    <g:Member name="Company" type="string"/>
    <g:Member name="Blog" type="Uri"/>
    <g:Member name="Location" type="string"/>
    <g:Member name="Email" type="string"/>
    <g:Member name="Hireable" type="bool"/>
    <g:Member name="Bio" type="string"/>
    <g:Member name="PublicRepositories" type="int" json="public_repos"/>
    <g:Member name="PublicGists" type="int"/>
    <g:Member name="Followers" type="int"/>
    <g:Member name="Following" type="int"/>
    <g:Member name="CreatedAt" type="DateTime"/>
    <g:Member name="UpdatedAt" type="DateTime"/>

  </g:Model>

  <g:Model name="gi:AuthorizedUser">

    <g:Inherits name="gi:User"/>
    
    <g:Member name="TotalPrivateRepositories" type="int" json="total_private_repos"/>
    <g:Member name="OwnedPrivateRepositories" type="int" json="owned_private_repos"/>
    <g:Member name="PrivateGists" type="int"/>
    <g:Member name="DiskUsage" type="long"/>
    <g:Member name="Collaborators" type="int"/>
    <g:Member name="Plan" type="gi:UserPlan"/>
  
  </g:Model>

  <g:Model name="gi:UserPlan">
      
    <g:Member name="Name" type="string"/>
    <g:Member name="Space" type="long"/>
    <g:Member name="PrivateRepositories" type="int" json="private_repos"/>
    <g:Member name="Collaborators" type="int"/>

  </g:Model>

</g:Models>
